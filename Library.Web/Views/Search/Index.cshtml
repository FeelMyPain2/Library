@model IEnumerable<Book>

@{
    ViewData["Title"] = "Search result";
}

<h1>Search result</h1>

@if (Model.Count() == 0)
{
    <p>Nothing found</p>
}
else
{
    <ul style="list-style-type:none;">
        @foreach (var book in Model)
        {
            <li><a href="@Url.Action("Index","Book",new {id = book.Id })">@book.Title</a></li>
        }
    </ul>
}


